        <Button
            android:id="@+id/btn_webGraph"
            style="?android:textAppearanceSmall"
            android:layout_width="wrap_content"
            android:layout_height="50dp"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="10dp"
